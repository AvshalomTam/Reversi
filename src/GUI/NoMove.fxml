<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.NoMoveController">
   <children>
      <Label fx:id="result" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="55.0" text="Uh oh! No possible moves." textFill="RED" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="73.0" text="Play passed to other player." textFill="RED" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" layoutY="117.0" mnemonicParsing="false" onAction="#continueGame" text="OK" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" />
   </children>
</AnchorPane>
